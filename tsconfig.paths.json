{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@layouts": [
                "./layouts/index.ts"
            ],
            "@layouts/*": [
                "./layouts/*"
            ],
            "@components": [
                "./components/index.ts"
            ],
            "@components/*": [
                "./components/*"
            ],
            "@pages": [
                "./pages/index.ts"
            ],
            "@pages/*": [
                "./pages/*"
            ],
            "@router": [
                "./router/index.ts"
            ],
            "@router/*": [
                "./router/*"
            ]
        }
    }
}